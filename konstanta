#include <stdio.h>

void main(){
    const double PHI = 3.14;
    const char JENIS_KELAMIN = 'P';
    const int VERSI = 10;

    printf("isi konstanta PI adalah %.2f\n", PHI);
    printf("isi konstanta JENIS_KELAMIN adalah %c\n", JENIS_KELAMIN);
    printf("isi konstanta VERSI adalah %i\n", VERSI);
}

#include <stdio.h>

void main(){

    int x, y;

    printf("Inputkan nilai x: ");
    scanf("%d", &x);

    printf("Inputkan nilai y: ");
    scanf("%d", &y);

    printf("Hasil x + y: %d\n", x + y);
    printf("Hasil x - y: %d\n", x - y);
    printf("Hasil x * y: %d\n", x * y);
    printf("Hasil x / y: %d\n", x / y);
    printf("Hasil x % y: %d\n", x % y);

}
